{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "type": "project",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "guzzlehttp/guzzle": "^6.3",

        "php7lab/eloquent": "v0.0.4",
        "php7lab/rest": "v0.0.4",
        "php7bundle/crypt": "v0.0.1",

        "symfony/console": "4.*",
        "symfony/serializer": "4.*",
        "symfony/validator": "4.*",

        "creocoder/yii2-flysystem": "^0.9.0",
        "deesoft/yii2-console": "~1.0",
        "yiisoft/yii2-queue": "^2.0",
        "mdmsoft/yii2-admin": "~2.0",
        "php7lab/yii2-legacy": "dev-task-1"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.1.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "php7lab/dev": "v0.0.2",
        "zndoc/rest-api": "v0.0.4",
        "symfony/debug-pack": "*"
    },
    "autoload": {
        "psr-4": {
            "Packages\\": "Packages"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "secure-http": false,
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "extra": {
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "vendor/{$vendor}/{$name}/": [
                "type:bower-asset",
                "type:npm-asset"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "git",
            "url": "https://github.com/php7lab/dev"
        },
        {
            "type": "git",
            "url": "https://github.com/php7lab/eloquent"
        },
        {
            "type": "git",
            "url": "https://github.com/php7lab/rest"
        },
        {
            "type": "git",
            "url": "https://github.com/php7lab/yii2-legacy"
        }
    ]
}
